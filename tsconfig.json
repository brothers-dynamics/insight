{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Node",
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "rootDirs": [
            ".",
            "./.svelte-kit/types",
            "./$houdini/types"
        ]
    },
    "ts-node": {
        "esm": true,
        "experimentalSpecifierResolution": "node"
    }
}